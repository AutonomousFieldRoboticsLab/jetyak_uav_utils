<launch>
	<node pkg="rosbag" type="record" name="rosbag_telemetry"
		args="record -o /dji_sdk/flight_status /dji_sdk/imu  /dji_sdk/attitude /dji_sdk/gps_position /dji_sdk/gps_health /dji_sdk/local_position /dji_sdk/velocity /dji_sdk/rc /dji_sdk/battery_state"
	</node>

	<node pkg="rosbag" type="record" name="rosbag_controls"
		args="record -o /joy /jetyak_uav_utils/uav_mode /jetyak_uav_utils/raw_cmd_pos_ENU /jetyak_uav_utils/raw_cmd_vel_FLU /dji_sdk/flight_control_setpoint_generic"
	</node>

	<node pkg="rosbag" type="record" name="rosbag_camera"
		args="record -o /raspicam_node/image_raw /raspicam_node/camera_info"
	</node>
</launch>
